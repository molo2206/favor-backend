    <td>
      <ul>
        <% subOrder.items.forEach(item => { %>
          <li><%= item.product.name %> (x<%= item.quantity %>)</li>
        <% }) %>
      </ul>
    </td>
    <td><%= subOrder.totalAmount.toFixed(2) %> â‚¬</td>
  </tr>
<% }) %>
